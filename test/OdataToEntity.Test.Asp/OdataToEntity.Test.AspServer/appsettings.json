{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "server.urls": "http://localhost:5000",

  "OdataToEntity": {
    "BasePath": "api",
    "Provider": "sqlserver",
    "ConnectionString": "Server=localhost;Database=Test_OData_Db;User Id=postgres;Password=postgres;Integrated Security=False",
    "UseRelationalNulls": true,
    "AllowCache": true
  },

  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5005"
      }
    }
  }
}
