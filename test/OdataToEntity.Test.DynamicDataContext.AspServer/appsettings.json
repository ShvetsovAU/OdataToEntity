{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "OdataToEntity": {
    "BasePath": "api",
    "Provider": "sqlserver",
    "ConnectionString": "Server=localhost;Database=Test_OData_Db;User Id=postgres;Password=postgres;Integrated Security=False",
    "UseRelationalNulls": true,
    "DefaultSchema": "dbo",
    "IncludedSchemas": [ "dbo" ], //, "test" ],
    "ExcludedSchemas": null,
    "Filter": "mapping"
    //,"InformationSchemaMappingFileName": "InformationSchemaMapping.json"
  }
}
